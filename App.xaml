<Application x:Class="Metal_Code.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:databinding="clr-namespace:Metal_Code"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <databinding:CostConverter x:Key="CostConverter"/>
        <databinding:MassConverter x:Key="MassConverter"/>
        <databinding:DestinyConverter x:Key="DestinyConverter"/>
        <databinding:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
    </Application.Resources>
</Application>
