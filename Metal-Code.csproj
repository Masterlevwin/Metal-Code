    <Resource Include="Images\save2.png" />
    <Resource Include="Images\save3.png" />
    <Resource Include="Images\save4.png" />
    <Resource Include="Images\update.png" />
    <Resource Include="Images\update4.png" />
    <Resource Include="Images\xlsx_load.png" />
    <Resource Include="Images\xls_load.png" />
    <Resource Include="laser_logo.jpg" />
    <Resource Include="Themes\appTheme.xaml" />
    <Resource Include="Themes\laserTheme.xaml" />
  </ItemGroup>

</Project>
